<%= render "layouts/errors", :object => @product %>

<%= form_for @product do |f| %>
<p>
  <%= f.label :name %>
  <%= f.text_field :name %>
</p>
<p>
  <%= f.label :cost %>
  <%= f.text_field :cost %>
</p>
<p>
  <%= f.label :origin %>
  <%= f.text_field :origin %>
</p>
<p>
  <%= f.submit %>
</p>

<% end %>
